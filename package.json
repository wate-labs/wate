{
  "name": "wate",
  "description": "A tool for automated tests for web APIs",
  "version": "0.12.0-dev",
  "author": "Flo Schuessel @scflode",
  "bin": {
    "wate": "./bin/run"
  },
  "bugs": "https://github.com/wate-labs/wate/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.4.4",
    "@oclif/plugin-help": "^5.1.17",
    "@types/http-string-parser": "0.0.30",
    "@types/mock-fs": "^4.13.1",
    "ajv": "^8.11.2",
    "axios": "1.2.1",
    "date-fns": "^2.29.3",
    "exceljs": "^4.3.0",
    "http-string-parser": "0.0.6",
    "js-yaml": "^4.1.0",
    "jsonata": "^1.8.6",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.10",
    "@types/nunjucks": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "chai": "^4",
    "eslint": "^8.29.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "globby": "^11",
    "mocha": "^10",
    "mock-fs": "^5.2.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wate-labs/wate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "dirname": "wate",
    "bin": "wate",
    "topics": {
      "create": {
        "description": "create requests, environments or suites"
      },
      "list": {
        "description": "list requests, environments, tests or suites"
      },
      "run": {
        "description": "run requests, tests or suites"
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "macos": {
      "identifier": "org.wate.cli"
    }
  },
  "repository": "wate-labs/wate",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test:single": "mocha --forbid-only",
    "test": "mocha --parallel --forbid-only 'test/**/*.test.ts'",
    "version": "oclif readme && git add README.md",
    "build:tarballs": "npx oclif pack tarballs",
    "build:macos": "npx oclif pack macos"
  },
  "types": "dist/index.d.ts"
}
